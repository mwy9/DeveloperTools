<Window x:Class="LockScreenPrevent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LockScreenPrevent"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="360">
    <Grid>
        <Button x:Name="btnTimerStart" Content="Start Timer" HorizontalAlignment="Left" Margin="10,0,0,20" VerticalAlignment="Bottom" Height="75" Width="150" Click="btnTimerStart_Click"/>
        <Button x:Name="btnTimerStop" Content="Close Timer" HorizontalAlignment="Right" Margin="0,0,10,20" VerticalAlignment="Bottom" Height="75" Width="150" Click="btnTimerStop_Click"/>

    </Grid>
</Window>
